@model IEnumerable<MortgageHelperModels.RatingListItem>

@{
    ViewBag.Title = "Rating Index";
}
<br />
<h2>Compare Properties</h2>
<br />
<p>Here is a list of all of your properties with their auto-generated ratings. These will be removed whenever you remove a property from your list.</p> 



<br />

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RatingActual)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.RatingActual)
        </td>

    </tr>
}

</table>
